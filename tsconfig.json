{
    "compilerOptions": {
        "target": "es2022",
        "module": "commonjs",
        "rootDir": "./src",
        "outDir": "./dist",
        "baseUrl": ".",
        "paths": {
        },
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
    }
}